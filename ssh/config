Host *
    IdentitiesOnly yes
    ExitOnForwardFailure yes
    Protocol 2
    ServerAliveInterval 400
    Port 22
    Compression yes
    CompressionLevel 7
    Cipher blowfish
    ControlMaster auto
    ControlPath /tmp/ssh-%r@%h:%p
