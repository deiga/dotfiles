### PRIVATE ###

Host home
    User deiga
    HostName 89.27.99.220

Host patch-tag.com
    User deiga
    Port 22
    Hostname patch-tag.com
    IdentityFile ~/.ssh/keys/patchtag_rsa
    TCPKeepAlive yes

Host github.com
    User git
    Hostname github.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/keys/github_rsa

Host bitbucket.org
    User git
    Hostname bitbucket.org
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/keys/github_rsa

Host loa
    User deiga
    Hostname leftoutsidealone.net
    Port 2255

Host nethack
    User nethack
    HostName nethack.fi
    PubkeyAuthentication false

Host heroku.com
    User git
    Hostname heroku.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/keys/heroku_rsa

### END ###

