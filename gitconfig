[core]
	excludesfile = ~/.gitignore
	autocrlf = input
	safecrlf = true
	editor = subl -w
[color]
	status = true
	diff = true
	branch = true
        ui = true
        grep = true
        showbranch = true
[merge]
	tool = opendiff
[apply]
	whitespace = nowarn
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an] \" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
    unstage = reset HEAD --
